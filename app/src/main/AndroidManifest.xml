<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.luatgiaothong">
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ClickItemActivity"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <action android:name="com.example.admin.luatgiaothong.ClickItemActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityBienBao" />
        <activity android:name=".ActivityListBienBao"
            android:windowSoftInputMode="stateHidden|adjustResize"/>
        <activity android:name=".ActivityDuongDayNong"
            android:windowSoftInputMode="stateHidden|adjustResize"/>
    </application>

</manifest>